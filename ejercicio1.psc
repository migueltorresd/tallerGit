Proceso ejercicio1
	definir indice, arreglo, n como entero;
	Dimension arreglo[4];
	

	
	
	para indice <-0 Hasta 3 con paso 1 Hacer 
		escribir "Ingrese por favor un numero";
		leer n;
		
		arreglo[indice]<- n;
		
	FinPara
	
	
	para indice <-0 Hasta 3 con paso 1 Hacer 
			
			
		escribir "[", indice, "] ->", arreglo[indice];
	FinPara
	
FinProceso

